<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <title>
#if ($label)
      $label.string | $server_name
#else
      Description of $uri | $server_name
#end
    </title>

    <link rel="stylesheet" href="/static/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/static/css/main.css" />

    <link rel="alternate" type="application/rdf+xml" href="$rdf_link?output=rdfxml" title="This page in RDF (XML)" />
    <link rel="alternate" type="text/turtle" href="$rdf_link?output=ttl" title="This page in RDF (Turtle)" />
  </head>

  <body class="browser">

    <!-- Navigation -->
      <div class="navbar navbar-inverse" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">#nowplaying</a>
        </div>
        <div class="collapse navbar-collapse navbar-right">
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li><a href="/static/docs">Documentation</a></li>
            <li><a href="/stats">Statistics</a></li>
            
            #if (!$classmap_links2.isEmpty())
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Data Set <b class="caret"></b></a>
                <ul class="dropdown-menu">

                #foreach ($aClassMap in $classmap_links2.keySet())
                  <li><a href="$classmap_links2.get($aClassMap)">$aClassMap</a></li>
                #end

                </ul>
              </li>
            #end

            <li><a href="/snorql">Snorql</a></li>
          </ul>
        </div>
      </div>
    </div>

    <!-- Title -->
    <div id="title">
      <div class="container">
        <h1>
          #if ($label)
            $label.string
          #else
            Description of $uri
          #end
        </h1>
        <p class="lead">Resource URI: $uri</p>
      </div>
    </div>

    <!-- Content -->
    <div id="content" class="container resource">

      <!-- List of Resource Classes -->
      <ol class="breadcrumb">
        #if (!$classmap_links.isEmpty())
          #foreach ($classmap in $classmap_links.keySet())
            <li><a href="$classmap_links.get($classmap)">All $classmap</a></li>
          #end
        #end
      </ol>

      <!-- Properties -->
      #if (!$properties.isEmpty())
        <table class="queryresults">
          <tr><th>Property</th><th>Value</th></tr>

          #foreach ($property in $properties)
            #if ($velocityCount % 2 == 0)
            #set ($odd_even = "even")
            #else
            #set ($odd_even = "odd")
            #end
                    <tr class="$odd_even">
                      <td class="property">
            #if ($property.isInverse())
                        <small>is</small>
            #end
            #if ($property.propertyPrefix())
                        <a class="uri" href="$property.propertyURI()" title="$property.propertyURI()"><small>$property.propertyPrefix()</small>$property.propertyLocalName()</a>
            #else
                        <a class="uri" href="$property.propertyURI()" title="$property.propertyURI()"><small>?:</small>$property.propertyLocalName()</a>
            #end
            #if ($property.isInverse())
                        <small>of</small>
            #end
                      </td>
                      <td>
            #if ($property.isImg() && $property.value().isURI())
                        <a href="$property.value().URI"><img src="$property.value().URI" class="depiction" /></a>
            #elseif ($property.valueQName())
                        <a class="uri" href="$property.value().URI">$property.valueQName()</a>
            #elseif ($property.value().isURI())
                        <small>&lt;</small><a class="uri" href="$property.value().URI">$property.value().URI</a><small>&gt;</small>
            #elseif ($property.value().isBlank())
                        <span class="blank">_:$property.value().BlankNodeLabel</span>
            #elseif ($property.value().isLiteral())
                        <span class="literal">$property.value().LiteralLexicalForm
            #if ($property.value().LiteralDatatype)
                        <small>($property.datatypeQName())</small>
            #end
            #if ($property.value().LiteralLanguage != "")
                        <small>($property.value().LiteralLanguage)</small>
            #end
                        </span>
            #end
                      </td>
                    </tr>
          #end

        </table>

        <!-- Limit -->
        #if ($limit_per_property_bridge)
              <p class="limit">The server is configured to display only a limited number of values (limit per property bridge: $limit_per_property_bridge).</p>
        #end

      #else
            <p>No textual description available.</p>
      #end


      <!-- Metadata -->
      #if ($metadata)
      <h3>Metadata</h3>
      <div id="metadata-tables">
        #set($bnidCount = 0)
        #set($idadd = 0)
        #listProperties ($metadataroot $metadata) 
      </div>
      #end


    </div>
    

    <!-- Footer -->
    <div id="footer" class="container">
      <div class="row">

        <div class="col-sm-6 dbis">
          <strong>DBIS</strong><br />
          Database and Information Systems<br />
          Institute of Computer Science<br />
          University of Innsbruck<br />
          <a href="http://dbis-informatik.uibk.ac.at/">http://dbis-informatik.uibk.ac.at/</a>
        </div>

        <div class="col-sm-6 description">
          <a href="$rdf_link" title="RDF data">RDF data <img src="${home_link}snorql/rdf_flyer.24.gif" alt="[RDF data]" /></a>
        </div>

      </div>
    </div>

    <!-- JavaScript -->
    <script type="text/javascript" src="/static/js/vendor/jquery.js"></script>
    <script type="text/javascript" src="/static/js/bootstrap.min.js"></script>
  </body>
</html>
