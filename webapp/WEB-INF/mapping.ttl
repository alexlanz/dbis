@prefix map: <#> .
@prefix db: <> .
@prefix vocab: <vocab/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d2r: <http://sites.wiwiss.fu-berlin.de/suhl/bizer/d2r-server/config.rdf#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .

@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix mo: <http://purl.org/ontology/mo/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix mbp: <http://musicbrainz.org/ns/mmd-1.0#> .


<> a d2r:Server;
        d2r:baseURI <http://138.232.66.90/>;
        d2r:port 8080;
        .


map:database a d2rq:Database;
	d2rq:jdbcDriver "com.mysql.jdbc.Driver";
	d2rq:jdbcDSN "jdbc:mysql://localhost/dbis";
	d2rq:username "dbis";
	d2rq:password "dbispwd";
	jdbc:autoReconnect "true";
	jdbc:zeroDateTimeBehavior "convertToNull";
	.

# Table listeningto (n:m)
map:listeningto__link a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:users;
	d2rq:property vocab:listeningto;
	d2rq:refersToClassMap map:tracks;
	d2rq:join "listeningto.user => users.id";
	d2rq:join "listeningto.track => tracks.id";
	.

# Table makers
map:makers a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "makers/@@makers.id@@";
	d2rq:class vocab:makers;
	d2rq:classDefinitionLabel "makers";
	.
map:makers__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:makers;
	d2rq:property rdfs:label;
	d2rq:pattern "makers #@@makers.id@@";
	.
map:makers_name a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:makers;
	d2rq:property vocab:makers_name;
	d2rq:propertyDefinitionLabel "makers name";
	d2rq:column "makers.name";
	.
map:makers_musicbrainzId a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:makers;
	d2rq:property mbp:id;
	d2rq:propertyDefinitionLabel "makers musicbrainzId";
	d2rq:column "makers.musicbrainzId";
	d2rq:datatype xsd:integer;
	.
map:maker_type a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:makers;
	d2rq:property dc:type;
	d2rq:propertyDefinitionLabel "makertypes name";
	d2rq:column "makertypes.name";
	d2rq:join "makers.type = makertypes.id";
	.

# Table tracks
map:tracks a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "tracks/@@tracks.id@@";
	d2rq:class mo:Track;
	d2rq:classDefinitionLabel "tracks";
	.
map:tracks__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tracks;
	d2rq:property rdfs:label;
	d2rq:pattern "tracks #@@tracks.id@@";
	.
map:tracks_title a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tracks;
	d2rq:property dc:title;
	d2rq:propertyDefinitionLabel "tracks title";
	d2rq:column "tracks.title";
	.
map:tracks_musicbrainzId a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tracks;
	d2rq:property mbp:id;
	d2rq:propertyDefinitionLabel "tracks musicbrainzId";
	d2rq:column "tracks.musicbrainzId";
	d2rq:datatype xsd:integer;
	.
map:tracks_maker__ref a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:tracks;
	d2rq:property vocab:tracks_maker;
	d2rq:refersToClassMap map:makers;
	d2rq:join "tracks.maker => makers.id";
	.

# Table users
map:users a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "users/@@users.id@@";
	d2rq:class foaf:Person;
	d2rq:classDefinitionLabel "users";
	.
map:users__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:users;
	d2rq:property rdfs:label;
	d2rq:pattern "users #@@users.id@@";
	.
map:users_name a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:users;
	d2rq:property foaf:name;
	d2rq:propertyDefinitionLabel "users name";
	d2rq:column "users.name";
	.

